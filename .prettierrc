{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
	  { "files": "*.svelte", "options": { "parser": "svelte", "singleQuote": false } },
	  { "files": "*.ts", "options": { "parser": "typescript" } },
	  { "files": "*.scss", "options": { "parser": "scss" } }
	]
  }